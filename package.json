{
  "name": "codeql-learning-catalog",
  "version": "2.0.0",
  "description": "CodeQL Learning Catalog",
  "scripts": {
    "start": "next start",
    "build": "npm run build:webpack && npm run build:next",
    "build:next": "next build",
    "build:webpack": "webpack --config webpack.prod.js",
    "export": "next export",
    "dev": "concurrently \"npm:dev:*\"",
    "dev:next": "next dev",
    "dev:webpack": "webpack --watch --config webpack.dev.js",
    "debug": "concurrently \"npm:debug:next\"  \"npm:dev:webpack\"",
    "debug:next": "NODE_OPTIONS='--inspect' next dev",
    "test": "concurrently \"npm:test:jest\" \"npm:test:prettier\"",
    "test:prettier": "prettier -c \"**/*.{ts,tsx,js,mjs,scss,yml,yaml}\" \"!.github/styles/**/*.{yml,yaml}\"",
    "test:jest": "jest",
    "prettier:fix": "prettier -w \"**/*.{ts,tsx,js,mjs,scss,yml,yaml}\" \"!.github/styles/**/*.{yml,yaml}\"",
    "markdown": "markdownlint 'docs/**/*.md' -c .github/linters/.markdownlint.json --ignore-path '.github/linters/.markdownlintignore' -i ./docs/index.md",
    "accessibility": "markdownlint $MARKDOWN_CHANGES -c .github/linters/.accessibilitylint.json --ignore-path '.github/linters/.accessibilitylintignore' -i ./docs/index.md -r .github/linters/accessibility-lint-rules"
  },
  "dependencies": {
    "@primer/css": "^19.1.0",
    "@primer/octicons": "^16.1.1",
    "@primer/octicons-react": "^16.0.0",
    "@primer/react": "^34.3.0",
    "@svgr/webpack": "^6.2.0",
    "babel-plugin-superjson-next": "^0.4.2",
    "classnames": "^2.3.1",
    "date-fns": "^2.30.0",
    "date-fns-tz": "^1.3.0",
    "fuse.js": "^6.5.3",
    "gray-matter": "^4.0.3",
    "hastscript": "^7.0.2",
    "js-yaml-loader": "^1.2.2",
    "mermaid": "^9.1.6",
    "next": "^12.0.7",
    "next-mdx-remote": "^3.0.5",
    "programming-languages-logos": "^0.0.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-string-replace": "^0.4.4",
    "rehype-autolink-headings": "^6.1.0",
    "rehype-slug": "^5.0.0",
    "rehype-stringify": "^9.0.2",
    "rehype-urls": "^1.1.1",
    "remark": "^14.0.1",
    "remark-gemoji": "^7.0.1",
    "remark-gfm": "^3.0.1",
    "remark-github": "^11.2.2",
    "remark-parse": "^10.0.1",
    "remark-rehype": "^10.1.0",
    "remark-toc": "^8.0.1",
    "rss": "^1.2.2",
    "sass": "^1.54.5",
    "slugify": "^1.6.5",
    "strip-indent": "^4.0.0",
    "styled-components": "^5.3.5",
    "superjson": "^1.8.0",
    "swr": "^1.3.0",
    "unified": "^10.1.1",
    "unist-util-visit": "^4.1.0"
  },
  "devDependencies": {
    "@types/anchor-js": "^4.2.0",
    "@types/d3": "^6.7.0",
    "@types/gtag.js": "^0.0.10",
    "@types/jest": "^28.1.8",
    "@types/mermaid": "^8.2.7",
    "@types/primer__octicons": "^16.1.0",
    "@types/react": "^17.0.37",
    "@types/react-dom": "^18.0.5",
    "anchor-js": "^4.3.1",
    "babel-jest": "^28.1.1",
    "babel-plugin-styled-components": "^2.0.2",
    "concurrently": "^7.0.0",
    "copy-webpack-plugin": "^10.2.4",
    "d3": "^6.7.0",
    "directory-tree-webpack-plugin": "^1.0.2",
    "freshlinks": "0.1.1",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.1",
    "js-yaml": "^4.1.0",
    "lunr": "^2.3.9",
    "markdownlint-cli": "^0.22.0",
    "nodemon": "^2.0.15",
    "prettier": "^2.8.7",
    "rss-to-json": "^2.0.2",
    "ts-loader": "^9.3.1",
    "typescript": "^4.2.4",
    "webpack": "^5.0.0",
    "webpack-cli": "^4.0.0",
    "webpack-dev-server": "^4.0.0",
    "webpack-merge": "^5.8.0"
  },
  "overrides": {
    "trim": "^1.0.1"
  }
}
